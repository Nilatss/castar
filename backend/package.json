{
  "name": "castar-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:migrate:local": "wrangler d1 execute castar-db --local --file=migrations/0001_initial.sql",
    "db:migrate:remote": "wrangler d1 execute castar-db --remote --file=migrations/0001_initial.sql",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "hono": "^4.7.0",
    "jose": "^6.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250214.0",
    "typescript": "~5.9.2",
    "wrangler": "^4.0.0"
  }
}
